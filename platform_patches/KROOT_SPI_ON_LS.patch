+
+       /*  target:1Mhz Frequency with i2c1 controller (100Mhz oscillator):
+        cnt = 100Mhz / 1Mhz = 100
+        hcnt + lcnt = 100
+        hcnt ~= lnct = 100/2 ~= 50
+               hcnt = hcnt - Controller specific adjustment
+               lcnt = lcnt - Controller Specific adjustment
+               dev->fp_hcnt = 50;
+        dev->fp_lcnt =  50; */
+        dev->fp_hcnt = 45;
