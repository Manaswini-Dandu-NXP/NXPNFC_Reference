+               cnt = 32Mhz / 1Mhz = 32
+               hcnt + lcnt = 32
+               hcnt ~= lnct = 32/2 ~= 16
+               hcnt = hcnt - Controller specific adjustment
+               lcnt = lcnt - Controller Specific adjustment
+               dev->fp_hcnt = 10;
