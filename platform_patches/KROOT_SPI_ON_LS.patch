+
+               /*  target:3.4Mhz Frequency with i2c1 controller (100Mhz oscillator):
+               cnt = 106Mhz / 3.4Mhz = ~30
+               hcnt + lcnt = ~30
+               hcnt ~= lnct = 3/2 ~= 15
+               hcnt = hcnt - Controller specific adjustment
+               lcnt = lcnt - Controller Specific adjustment
+               */
+        dev->hs_hcnt = 8;
+        dev->hs_lcnt = 8;
+        dev_err(&pdev->dev,
+           "##### High speed mode set: freq = %d", t->bus_freq_hz);
+               break;
+       case 400000:
+               /*  target:400KHz Frequency with i2c1 controller (100Mhz oscillator):
+               cnt = 100Mhz / .4Mhz = 250
+               hcnt + lcnt = 250
+               hcnt ~= lnct = 250/2 ~= 125
+               hcnt = hcnt - Controller specific adjustment
+               lcnt = lcnt - Controller Specific adjustment
+               */
+               dev->fs_hcnt = 125;
+               dev->fs_lcnt = 123;
+               break;
+       default:
+               dev_err(&pdev->dev,
+            "##### Standard mode set: freq = %d", t->bus_freq_hz);
+               break;
+       }
+
